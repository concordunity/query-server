<% var pageCount = 1; %>
<% for (var i = 0; i < this.length; i++) { %>
    <li class="nav-header type-<%= this[i].type %>" data-toggle="collapse" data-target="#tree-group-<%= i %>">
        <%= this[i].name %> (<%= this[i].pages.length %>)
    </li>
    
    <div id="tree-group-<%= i %>" class="collapse">
    <% for (var j = 0; j < this[i].pages.length; j++) { %>

        <li class="page-<%= pageCount %>">
            <a href="#<%= pageCount %>">第 <%= pageCount %> 页</a>
        </li>
        <% pageCount++; %>
    <% } %>
    </div>
<% } %>
