<head>
<title>存量报关单证电子档案管理系统 - 打印确认</title>
</head>
<body>
<div class="container-fluid">
  <div class="row-fluid">
    <div id="alerts">
  </div>
<div id="pdf-holder"></div>
</div>
<script type='text/javascript' src='../steal/steal.js'></script>
<script type='text/javascript'>
steal('jquery/dom/fixture', 'jquery/dom/route', 'docview/fixtures', 'docview/alerts', 'docview/showpdf').then(function($) {

      // Client State
      var alert_state = new $.Observe({
          alert: {
            type: "info",
            heading: "Herp",
            message: "derp"
          }
      });
      $('#alerts').docview_alerts({clientState: alert_state});
  var referrer = document.referrer.toLowerCase();
  var doc_id = ""
  var url = document.location.href;
  var pos = url.indexOf('?');

  if (pos != -1) {
    doc_id = url.substring(pos + 1);
  }
  if (doc_id) {
    $('#pdf-holder').docview_showpdf({doc_id : doc_id, doc_type: 'testify' });
//    $('#pdf-holder').docview_showpdf('printDoc');
//    $.post('/print/printpdf', { doc_type: 'testify', doc_id : doc_id }, function(data) { $('#pdf-holder').html(data); });
  } else {
    alert_state.attr('alert', {
             type: "error",
          heading: "错误提示：",
          message: "您的请求有错误。" + referrer + " "  + doc_id
    });
  } 
});
</script>
</body>
</html>
