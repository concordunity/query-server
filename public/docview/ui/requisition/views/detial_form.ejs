<tr class="detial_form" >
<td colspan='10' style="border:2px solid #cccc00;"   >
<!--startprint-->
<div style="margin: 30px 100px 30px 100px;" class="well">	
<form onsubmit="return false;">
<span id='print_detial_form_<%= model.id%>' >
<legend>详细信息(流水号：<%= model.serial_number%>)</legend>
<table class="table table-condensed table-striped text-decoration-right"   >
<tr>
	<td>业务点:</td>

<%
var subjection_org = orgJsonDictionary[model.org]
if(model.org == null || model.org == '' || model.org == "2200"){
    subjection_org = "总关"; 
}
%>
	<td><%= subjection_org + "(" + model.org  + ")"  %>  </td> 
	<td>抽单原由:</td>
	<td><%= model.application_originally %></td>
</tr>
<tr>
	<td>科室:</td>
	<td><%= model.department_name %>  </td> 
	<td>状态:</td>
	<td><%= scene_lent_paper_documentJsonDictionary[model.status.toString()] %></td>
</tr>
<tr>
	<td>存档地点:</td>

<%
var storage_sites = orgJsonDictionary[model.storage_sites]
if(model.storage_sites == null || model.storage_sites == '' || model.storage_sites == "2200"){
    storage_sites= "总关"; 
}
%>

	<td><span class=''><%= storage_sites  %></span></td> 
	<td>借阅单证数量:</td>
	<%
		var n = 0;
		for(var s=0;s < model.requisition_details.length;s++){
			!model.requisition_details[s].is_check && n++;
		}
	%>
	<td>
	<%= n %>
	<td>
</tr>
<tr>
	<td>申请人员:</td>
	<td><%= $.display_user(model.apply_staff,model.apply_staff_fullname) %></td>
	<td>申请时间:</td>
	<td><%= $.date(model.created_at).format("yyyy-MM-dd hh:mm:ss") %></td>
</tr>
<tr>
	<td>审批科长:</td>
	<td><%= $.display_user(model.approving_officer,model.approving_officer_fullname)  %></td>
	<td>科长审批时间:</td>
	<td><%= $.date(model.approval_time).format("yyyy-MM-dd hh:mm:ss") %></td>
</tr>
<tr>
	<td>审批关长:</td>
	<td><%= $.display_user(model.two_approvers,model.two_approvers_fullname)  %></td>
	<td>关长审批时间:</td>
	<td><%= $.date(model.two_approver_time).format("yyyy-MM-dd hh:mm:ss") %></td>
</tr>
<% if(model.termination_instructions){ %>
<tr>
<td>退回原因：</td>
<td><%= model.termination_instructions%></td>
<td></td>
<td></td>
</tr>
<% } %>

<tr>
	<td>档案管理人员:</td>
	<td><%= $.display_user(model.registration_staff,model.registration_staff_fullname) %></td>
	<td>档案登记时间:</td>
	<td><%= $.date(model.check_in_time).format("yyyy-MM-dd hh:mm:ss") %></td>
</tr>

<tr>
	<td>核销人员:</td>
	<td><%= $.display_user(model.write_off_staff, model.write_off_staff_fullname) %></td>
	<td>核销时间:</td>
	<td><%= $.date(model.write_off_time).format("yyyy-MM-dd hh:mm:ss") %></td>
</tr>
<td colspan="4">
</td>
</tr>
</table>
<div class="well"  >
<table class="table " >
		<caption><legend>抽单信息</legend></caption>
		<tr>
			<th>报关单号</th>
			<th>理单号码</th>
			<th>状态</th>
			<% if(($.inArray(model.status,[13,14]) != -1 || action == 'register')){ %><th>查无此单</th><% } %>
		</tr>
	<tbody>
	<% 
		var t = 0;
		for(var j = 0; j < model.requisition_details.length; j++) { %>
			<% var is_checked = model.requisition_details[j].is_check; %>
			<tr class='<%= !!!is_checked  ? "" : "print_requisition_details  noprint" %>'>
			<td><span class="<%=is_checked ? '': 'single_card_number' %>"><%= model.requisition_details[j].single_card_number%></span></td>
			<td><span class=""><%= model.requisition_details[j].rationale_single_number%> </span></td>
			<td><span class=""><%= model.requisition_details[j].status == 1 ? "已经电子化" : "尚未电子化"%> </span></td>
			<td> 
			<% if(action == 'register'){ %>
				<input type="checkbox" value='<%= model.requisition_details[j].id %>' />
			<%	}else if($.inArray(model.status,[13,14]) != -1){
					is_checked && t++;
					%>
					<input type="checkbox" value='<%= model.requisition_details[j].id %>' <%= is_checked ? 'checked' : ''  %> disabled='disabled'  />
				<%}else{ %>
					
			<%	} 
			%>
			</td>
		</tr>
	<% } %>
	</tbody>
</table>
</div>
</span>
<!---endprint-->
	<br />
	<%
	var name_map = {
		10:{
			fullname:model.apply_staff_fullname,
			text:'撤销审批，重新指定审批科长'
		},
		11:{
			fullname:model.approving_officer_fullname,
			text:'撤销审批，重新指定审批关长'
		},
		21:{
			fullname:model.apply_staff_fullname,
			text:'重新指定审批科长'
		},
		22:{
			fullname:model.approving_officer_fullname,
			text:'重新指定审批关长'
		}
	};
	%>
	<% var user = this.ctx.options.clientState.attr('user') %>
	

	<% if (this.tag && action != 'register') {%>
	全部打印<input type='checkbox' class='print_all'/>
		<% }%>
	<br />
	<br />
	<!-- Accept -->
	<%
	if($.inArray(action,['approval','approval_guan','register','write_off']) != -1){
	%>
		<%
		if(action == 'approval' && model.status == 10 ){
		%>
			<b>审批关长:</b>
			<div class="gld_user_select require_select" data-type='gld_users' ></div>
			<br />
		<%
		}
		%>
		<button type="button" class="btn btn-primary btn-accept" <%= (action == 'write_off') ? 'disabled':'' %> ><%= (action.indexOf('approval') == -1) ? "完成" : "通过" %></button>
	<%	
	}
	%>
	<!-- Accept end -->

	<!-- Reject -->
	<%
	if(action.indexOf('approval') != -1 || action == 'write_off') {
		%><button type="button" class="btn btn-danger btn-reject" rel="popover" data-placement="right" data-content="请输入退回理由" data-original-title="退回" >退回</button>	
		<%
	}
	%>

	<!-- Reject end -->
	<!-- Revoaction -->
	<%if(action == 'requisition_history' &&  name_map[model.status] && name_map[model.status].fullname == user.fullname){ %>
		<%if( $.inArray(model.status,[10,21]) != -1 ){%>
			<b>审批科长:</b>
			<div class="user_select require_select" data-type='kz_users' ></div>
		<%}%>
		<%if( $.inArray(model.status,[11,22]) != -1 ){%>
			<b>审批关长:</b>
			<div class="user_select require_select" data-type='gld_users' ></div>
		<%}%>
		<br />
		<button type="button" class="btn btn-danger btn-revocation" ><i class="icon-edit icon-white"></i> <%= name_map[model.status].text %></button>
		<%if(!model.approving_officer){ %>
			<button type="button" class="btn btn-danger btn-delete"><i class="icon-trash icon-white" ></i> 删除</button>
		<%}%>	
	<%}%>	
	<!-- Revocation end -->


	<%if(action == 'requisition_history' &&  name_map[model.status] && name_map[model.status].fullname == user.fullname && model.status == 11){ %>
		<button type="button" class="btn btn-danger btn-reject-cancel" rel="popover" data-placement="right" data-content="请输入撤销申请的理由" data-original-title="撤销申请" >撤销申请</button>	
	<% } %>


	<button type="button" class="btn btn-info btn-print"><i class="icon-print icon-white"></i> 打印</button>
	<button type="button" class="btn btn-cancel">取消</button>
	<br />
	<br />
	<div class="well well-check">	
		<legend>单证校验</legend>
		<span class="alert alert-info alert-tips">只有报关单号全部校验成功后才可以提交</span>
		<br />
		<br />
		<input class="txt-check" type="text" minlength='18' required  />
	</div>
</form>
</div>
</td>
</tr>
